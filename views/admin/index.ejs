<% include ../partials/header %>
<div class='container'>
	<div class='content col-md-12'>
	<% if (reservations.length > 0) { %>
		<% for (i=0; i<reservations.length; i++) { %>
				<div class='reservation_list'>
					<h1><%= reservations[i].name %></h1>
					<ul>
						<li>Password: <%= reservations[i].secret %></li>
						<li>Phone Number: <%= reservations[i].phone_num %></li>
						<li>Reservation Time: <%= reservations[i].time %></li>
					</ul>
				</div>
		<% } %>
  <% } %>
  </div>
 </div>
	
<% include ../partials/footer %>